var period = 2s
var every = 2s
var data = stream

|from()
  .database('asgbackup')
  .retentionPolicy('autogen')
  .measurement('drive_info')
  .where 
  .groupBy(*)
|window()
  .period(period)
  .every(every)
|count('bytes')
  .as ('count_bytes')
|alert()
  .message('Value: {{index .Fields "count_bytes"}}')
  .crit(lambda: "count_bytes" <  80)
  .slack()
  .log('/var/tmp/kapacitor/points.log')


